        "device": {
            "data_type": "string",
            "order": 0,
            "description": "Device IP/Hostname",
            "required": true
        },
        "port": {
            "data_type": "string",
            "order": 1,
            "description": "Device Port",
            "required": false,
            "default": "830"
        },
        "username": {
            "data_type": "string",
            "order": 3,
            "description": "Username",
            "required": true
        },
        "password": {
            "data_type": "password",
            "order": 4,
            "description": "Password",
            "required": true
        }
    },
